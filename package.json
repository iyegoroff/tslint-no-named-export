{
  "name": "tslint-no-named-export",
  "version": "0.0.2",
  "description": "TSLint rule that prohibits named exports",
  "main": "index.json",
  "scripts": {
    "preversion": "npm run lint && npm run build && npm t",
    "postversion": "git push && git push --tags",
    "build": "rm -rf dist && mkdir dist && tsc -p .",
    "lint": "tslint -p .",
    "watch": "npm run build -- --watch",
    "test": "tslint --test test/**/tslint.json"
  },
  "keywords": [
    "tslint",
    "no-named-export"
  ],
  "files": [
    "dist",
    "src",
    "test"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/tslint-no-named-export/issues"
  },
  "homepage": "https://github.com/iyegoroff/tslint-no-named-export#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/tslint-no-named-export.git"
  },
  "devDependencies": {
    "@types/node": "^12.0.3",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "tslint": "^5.x",
    "typescript": "^3.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "dependencies": {
    "array-flatten": "^2.1.2"
  }
}
